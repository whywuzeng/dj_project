
@{
    ViewBag.Title = "HRM项目进展";
}

<!-- Bootstrap-table -->
<link href="~/Scripts/bootstrp-table/bootstrap-table.css" rel="stylesheet" />
<!-- datetimepicker -->
<link href="~/Scripts/bootstrap-datetime/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<!-- fileinput -->
<link href="~/Scripts/fileinput/css/fileinput.min.css" rel="stylesheet" />

<!-- Custom Theme Style -->
<link href="~/Scripts/build/css/custom.min.css" rel="stylesheet">

<link href="~/Scripts/Hrm/css/hrmStyle.css" rel="stylesheet" />

<style>
    #myModal {
        top: 20%;
    }
</style>

<!-- 数据信息 -->
<div class="row" id="mainData">

    <!-- 查询 -->
    <div class="x_panel">
        <div class="x_title">
            <a class="collapse-link">
                <span style="color:#250486;font-size:14px;font-weight:bold;">查询条件：</span>
                <span class="pull-right"><i class="fa fa-chevron-up"></i></span>
            </a>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <form class="form-horizontal form-label-left" novalidate>

                <div class="item form-group">
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        年 度：
                        <select class="selectStyle" id="yearly"></select>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        月 度：
                        <select class="selectStyle2" id="monthly1">
                        </select>
                        —
                        <select class="selectStyle2" id="monthly2">
                        </select>
                    </div>
                </div>

            </form>
        </div>
    </div>

    <!-- 数据 -->
    <div class="x_panel">
        <div class="x_title">
            <div class="pull-left">
                <div class="btn-group">
                    <span class="btn btn-default btn-sm">菜单：</span>
                    <a href="#" id="btnChart" class="btn btn-default btn-sm"><i class="fa fa-bar-chart-o"></i>&nbsp;图表</a>
                    <a href="#" id="btnExp" class="btn btn-default btn-sm"><i class="fa fa-print"></i>&nbsp;导出</a>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <div class="row top_tiles">
                <table id="table1" data-classes="table table-hover"></table>
            </div>

        </div>
    </div>

</div>


<div class="modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false" data-keyboard="false">
    <div class="modal-dialog" style="height:50px;" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                <h4 class="modal-title" id="myModalLabel">提示：</h4>
            </div>
            <div class="modal-body">
                <span id="result" style="color:red;"></span>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal" style="margin-right:20px;"><i class="fa fa-check"></i>&nbsp;确定</button>
            </div>
        </div>
    </div>
</div>


<!-- jQuery -->
<script src="~/Scripts/vendors/jquery/dist/jquery.min.js"></script>

<!-- Bootstrap-datetime -->
<script src="~/Scripts/bootstrap-datetime/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/bootstrap-datetime/bootstrap-datetimepicker.zh-CN.js"></script>

<!-- Bootstrap-table -->
<script src="~/Scripts/bootstrp-table/bootstrap-table.js"></script>
<script src="~/Scripts/bootstrp-table/bootstrap-table-zh-CN.js"></script>

<script src="~/Scripts/Hrm/hrjs/Hr_ModelPro.js"></script>

<script>

    $(function () {

        jsObject.Setted.init();
    });


</script>

